{
	"name": "flipside",
	"version": "0.1.0",
	"private": true,"repository": {
        "type": "git",
        "url": "https://github.com/MichaelLisboa/flipside"
    },
    "homepage": "http://flipside.global",
    "engines": {
        "node": ">=8.0.0"
    },
	"dependencies": {
		"concurrently": "^5.0.0",
		"formik": "^1.5.8",
		"gh-pages": "^2.1.1",
		"nodemon": "^1.19.3",
		"react": "^16.9.0",
		"react-dom": "^16.9.0",
		"react-router-dom": "^5.1.2",
		"react-scripts": "5.0.1",
		"react-spring": "^8.0.27",
		"react-use-gesture": "^6.0.3",
		"uikit": "^3.2.1",
		"yes-https": "^1.0.0",
		"yup": "^0.27.0"
	},
	"scripts": {
		"predeploy": "npm run generate-build-version && npm run build",
		"deploy": "gh-pages -d build",
		"generate-build-version": "node generate-build-version",
		"start": "node server.js",
		"build": "react-scripts build",
		"server": "nodemon server.js",
		"local": "concurrently \"npm run server\" \"react-scripts start\""
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"proxy": "http://localhost:5000"
}
